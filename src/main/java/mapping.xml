<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="hibernate.artifact1.Employee" table="employee">
<id name="id" column="id" type="java.lang.Integer"></id>
<property name="firstName" column="firstname1" type="java.lang.String"></property>
<property name="lastName" column="lastname1" type="java.lang.String"></property>
<property name="salary" column="salary" type="java.lang.Integer"></property>

</class>



</hibernate-mapping>